{
  "name": "pokedex",
  "version": "0.1.0",
  "description": "this is a simple progressive webapp for list pokemons type and informations",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html"
  },
  "author": "@vitormalencar",
  "license": "MIT",
  "dependencies": {
    "object-assign": "4.1.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-redux": "7.1.1",
    "react-router": "5.1.2",
    "react-router-redux": "4.0.8",
    "redux": "4.0.4"
  },
  "devDependencies": {
    "autoprefixer": "9.6.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.26.7",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "connect-history-api-fallback": "1.6.0",
    "coveralls": "3.0.7",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "eslint": "6.5.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-watch": "6.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "isparta": "4.1.1",
    "mocha": "6.2.1",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "open": "6.4.0",
    "postcss-loader": "3.0.0",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "2.1.0",
    "replace": "1.1.1",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0",
    "style-loader": "1.0.0",
    "url-loader": "2.2.0",
    "webpack": "4.41.1",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
